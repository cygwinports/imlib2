DESCRIPTION="Enlightenment image loading library"
HOMEPAGE="http://www.enlightenment.org/"
SRC_URI="mirror://sourceforge/enlightenment/${P}.tar.gz"

CYGCONF_ARGS="--enable-x --enable-mmx --disable-static"
DOCS="COPYING-PLAIN"

PKG_NAMES="${PN} lib${PN} lib${PN}-devel"
PKG_HINTS="setup lib devel"
PKG_CONTENTS[0]='usr/bin/*.exe usr/share/'
PKG_CONTENTS[1]="usr/bin/*.dll usr/lib/${PN}/"
PKG_CONTENTS[2]='usr/bin/*-config usr/include/ usr/lib/lib* usr/lib/pkgconfig/'
